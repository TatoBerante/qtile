#!/bin/bash

ULRED="\e[4;31m"
LRED="\e[31m"
RED="\e[91m"

LGREEN="\e[32m"
GREEN="\e[92m"

LYELLOW="\e[33m"
YELLOW="\e[93m"

LBLUE="\e[34m"
BLUE="\e[94m"

LMAGENTA="\e[35m"
MAGENTA="\e[95m"

LCYAN="\e[36m"
CYAN="\e[96m"

GRAY="\e[37m"
WHITE="\e[97m"

ENDCOLOR="\e[0m"

echo -e ${LBLUE}LBLUE${ENDCOLOR}"\t "${BLUE}BLUE${ENDCOLOR}
echo -e ${LGREEN}LGREEN${ENDCOLOR}"\t "${GREEN}GREEN${ENDCOLOR}
echo -e ${LRED}LRED${ENDCOLOR} ${RED}RED${ENDCOLOR} ${ULRED}ULRED${ENDCOLOR}
echo -e ${LMAGENTA}LMAGENTA${ENDCOLOR} ${MAGENTA}MAGENTA${ENDCOLOR}
echo -e ${LCYAN}LCYAN${ENDCOLOR}"\t "${CYAN}CYAN${ENDCOLOR}
echo -e ${LYELLOW}LYELLOW${ENDCOLOR}"\t "${YELLOW}YELLOW${ENDCOLOR}
echo -e ${GRAY}GRAY${ENDCOLOR}"\t "${WHITE}WHITE${ENDCOLOR}
